{
  "name": "astral-location-services",
  "version": "0.1.0",
  "private": true,
  "description": "Verifiable geospatial computation service for Ethereum",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "npm run dev --workspace=@astral/compute-service",
    "dev:db": "docker-compose -f docker-compose.dev.yml up -d",
    "dev:db:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "lint": "npm run lint --workspaces --if-present",
    "typecheck": "npm run typecheck --workspaces --if-present"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
