# =============================================================================
# COMPUTE SERVICE (packages/compute-service)
# =============================================================================
# These are the only variables needed to run the compute service.

# Required
DATABASE_URL=postgresql://astral:astral@localhost:5432/astral
SIGNER_PRIVATE_KEY=  # Generate: node -e "console.log(require('ethers').Wallet.createRandom().privateKey)"

# Optional (defaults shown)
PORT=3000
CHAIN_ID=84532                    # 84532 = Base Sepolia, 8453 = Base Mainnet
NODE_ENV=development
ALLOWED_ORIGINS=*                 # Comma-separated list, e.g., https://app.astral.global,http://localhost:3000

# =============================================================================
# SDK / CLIENT APPS (packages/sdk-extensions)
# =============================================================================
# These are for applications that USE the Astral API, not for running the service.

# ASTRAL_API_URL=http://localhost:3000    # URL of Astral compute service
# RPC_URL=                                 # Ethereum RPC for onchain submission
# PRIVATE_KEY=                             # Wallet key for submitting attestations onchain
